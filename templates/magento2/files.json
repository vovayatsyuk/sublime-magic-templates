{
    "/Block/": [
        { "pattern": ".*\\.php", "path": "Block/Block.php" }
    ],
    "/Controller/Adminhtml/": [
        { "pattern": "NewAction\\.php", "path": "Controller/Adminhtml/Entity/NewAction.php" },
        { "pattern": "Edit\\.php", "path": "Controller/Adminhtml/Entity/Edit.php" },
        { "pattern": "Save\\.php", "path": "Controller/Adminhtml/Entity/Save.php" },
        { "pattern": "MassDelete\\.php", "path": "Controller/Adminhtml/Entity/MassDelete.php" },
        { "pattern": "Mass[A-Z].*\\.php", "path": "Controller/Adminhtml/Entity/MassStatus.php" },
        { "pattern": "Delete\\.php", "path": "Controller/Adminhtml/Entity/Delete.php" },
        { "pattern": "Validate\\.php", "path": "Controller/Adminhtml/Entity/Validate.php" },
        { "pattern": ".*\\.php", "path": "Controller/Adminhtml/Entity/Index.php" }
    ],
    "/Controller/": [
        { "pattern": "(Save|Post)\\.php", "path": "Controller/Entity/Save.php" },
        { "pattern": ".*\\.php", "path": "Controller/Entity/Index.php" }
    ],
    "/etc/": [
        { "pattern": "adminhtml/di\\.xml", "path": "etc/adminhtml/di.xml" },
        { "pattern": "adminhtml/events\\.xml", "path": "etc/adminhtml/events.xml" },
        { "pattern": "adminhtml/menu\\.xml", "path": "etc/adminhtml/menu.xml" },
        { "pattern": "adminhtml/routes\\.xml", "path": "etc/adminhtml/routes.xml" },
        { "pattern": "adminhtml/system\\.xml", "path": "etc/adminhtml/system.xml" },
        { "pattern": "frontend/di\\.xml", "path": "etc/frontend/di.xml" },
        { "pattern": "frontend/events\\.xml", "path": "etc/frontend/events.xml" },
        { "pattern": "frontend/page_types\\.xml", "path": "etc/frontend/page_types.xml" },
        { "pattern": "frontend/routes\\.xml", "path": "etc/frontend/routes.xml" },
        { "pattern": "frontend/sections\\.xml", "path": "etc/frontend/sections.xml" },
        { "pattern": "acl\\.xml", "path": "etc/acl.xml" },
        { "pattern": "config\\.xml", "path": "etc/config.xml" },
        { "pattern": "di\\.xml", "path": "etc/di.xml" },
        { "pattern": "events\\.xml", "path": "etc/events.xml" },
        { "pattern": "module\\.xml", "path": "etc/module.xml" },
        { "pattern": "widget\\.xml", "path": "etc/widget.xml" }
    ],
    "/Helper/": [
        { "pattern": ".*\\.php", "path": "Helper/Data.php" }
    ],
    "/Model/ResourceModel/": [
        { "pattern": "/Grid/Collection\\.php", "path": "Model/ResourceModel/Entity/Grid/Collection.php" },
        { "pattern": "/Collection\\.php", "path": "Model/ResourceModel/Entity/Collection.php" },
        { "pattern": ".*\\.php", "path": "Model/ResourceModel/Entity.php" }
    ],
    "/Model/": [
        { "pattern": ".*Repository\\.php", "path": "Model/EntityRepository.php" },
        { "pattern": "/Source/.*", "path": "Model/Config/Source/Entity.php" },
        { "pattern": ".*\\.php", "path": "Model/Entity.php" }
    ],
    "/Observer/": [
        { "pattern": ".*\\.php", "path": "Observer/Default.php" }
    ],
    "/Plugin/": [
        { "pattern": ".*\\.php", "path": "Plugin/Default.php" }
    ],
    "/Setup/": [
        { "pattern": "UpgradeSchema\\.php", "path": "Setup/UpgradeSchema.php" }
    ],
    "/Ui/": [
        { "pattern": "DataProvider/(.*/)?Form/.*Provider\\.php", "path": "Ui/DataProvider/Form/EntityProvider.php" },
        { "pattern": "DataProvider/.*Provider\\.php", "path": "Ui/DataProvider/EntityProvider.php" },
        { "pattern": "Column(s)?/.*Actions\\.php", "path": "Ui/Component/Listing/Columns/Actions.php" },
        { "pattern": "Column(s)?/.*\\.php", "path": "Ui/Component/Listing/Columns/Default.php" }
    ],
    "/view/adminhtml/": [
        { "pattern": "layout/.*_index\\.xml", "path": "view/adminhtml/layout/index.xml" },
        { "pattern": "layout/.*_edit\\.xml", "path": "view/adminhtml/layout/edit.xml" },
        { "pattern": "layout/.*_new\\.xml", "path": "view/adminhtml/layout/new.xml" },
        { "pattern": "ui_component/.*list(ing)?\\.xml", "path": "view/adminhtml/ui_component/listing.xml" },
        { "pattern": "ui_component/.*(form|new|edit)\\.xml", "path": "view/adminhtml/ui_component/form.xml" }
    ],
    "/view/": [
        { "pattern": "layout/.*\\.xml", "path": "view/base/layout/layout.xml" },
        { "pattern": "requirejs\\-config\\.js", "path": "view/base/requirejs-config.js" },
        { "pattern": "action.*\\-mixin\\.js", "path": "view/base/web/js/mixin/action-mixin.js" },
        { "pattern": "model.*\\-mixin\\.js", "path": "view/base/web/js/mixin/model-mixin.js" },
        { "pattern": "view.*\\-mixin\\.js", "path": "view/base/web/js/mixin/view-mixin.js" },
        { "pattern": "js/mixin/.*\\.js", "path": "view/base/web/js/mixin/model-mixin.js" },
        { "pattern": "js/action/.*\\.js", "path": "view/base/web/js/action.js" },
        { "pattern": "js/model/.*\\.js", "path": "view/base/web/js/model.js" },
        { "pattern": "js/view/.*\\.js", "path": "view/base/web/js/view.js" },
        { "pattern": "js/.*\\.js", "path": "view/base/web/js/default.js" }
    ],
    "/": [
        { "pattern": "composer\\.json", "path": "composer.json" },
        { "pattern": "README\\.md", "path": "README.md" },
        { "pattern": "registration\\.php", "path": "registration.php" },
        { "pattern": ".*\\.php", "path": "default.php" }
    ],
    "snippets": {
        "ifip": { "path": "/magento2/snippets/ifip" },
        "classname": { "path": "/php/snippets/classname" },
        "namespace": { "path": "/php/snippets/namespace" }
    },
    "@todo:@extend": {
        "php/rules.json": "*"
    }
}
